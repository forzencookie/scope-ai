{
  "name": "third-dashboard-design",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:clean": "rm -rf .next && next dev --turbopack",
    "dev:light": "next dev",
    "dev:limit": "NODE_OPTIONS='--max-old-space-size=2048' next dev --turbopack",
    "clean": "rm -rf .next",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:count": "echo 'ESLint disables:' && grep -r 'eslint-disable' src --include='*.ts' --include='*.tsx' | wc -l | tr -d ' '",
    "lint:report": "echo '=== ESLint Disable Report ===' && grep -roh 'eslint-disable[^*]*' src --include='*.ts' --include='*.tsx' | sed 's/.*@//' | sed 's/ .*//' | sort | uniq -c | sort -rn",
    "typecheck": "tsc --noEmit",
    "typecheck:count": "echo 'TypeScript errors:' && tsc --noEmit 2>&1 | grep 'error TS' | wc -l | tr -d ' '",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "functions:serve": "supabase functions serve",
    "functions:deploy": "supabase functions deploy",
    "functions:new": "supabase functions new",
    "db:types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.ts",
    "db:types:local": "supabase gen types typescript --local > src/types/database.ts",
    "prepare": "husky && husky install"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-email/components": "^1.0.6",
    "@react-email/render": "^2.0.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.87.1",
    "@tabler/icons-react": "^3.36.1",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.25",
    "html2canvas": "^1.4.1",
    "ical-generator": "^10.0.0",
    "jspdf": "^4.0.0",
    "jszip": "^3.10.1",
    "lucide-react": "^0.555.0",
    "next": "^16.0.7",
    "next-themes": "^0.4.6",
    "openai": "^6.10.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-markdown": "^10.1.0",
    "recharts": "^2.15.4",
    "remark-gfm": "^4.0.1",
    "resend": "^6.9.1",
    "sonner": "^2.0.7",
    "stripe": "^20.2.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "baseline-browser-mapping": "^2.9.19",
    "eslint": "^9",
    "eslint-config-next": "16.0.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1",
    "supabase": "^2.72.8",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "husky": "^8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npx eslint --fix --max-warnings 50"
    ],
    "*.{json,md}": [
      "npx prettier --write --ignore-unknown"
    ]
  }
}
